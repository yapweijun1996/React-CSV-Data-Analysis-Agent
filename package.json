{
  "name": "csv-data-analysis-agent",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "clean": "node --input-type=module -e \"import { rmSync } from 'fs'; rmSync('dist', { recursive: true, force: true });\"",
    "build": "npm run clean && npm run verify:schema && vite build",
    "preview": "vite preview",
    "test": "node --import tsx ./tests/runAllTests.ts",
    "test:integration": "node --import tsx ./tests/chatPlannerIntegration.test.ts",
    "verify:schema": "node --import tsx ./scripts/verifySchema.ts",
    "verify:schema:plan-state": "node --import tsx ./scripts/verifySchema.ts --scope=plan_state_update",
    "precommit": "npm run verify:schema:plan-state"
  },
  "dependencies": {
    "@google/genai": "^1.28.0",
    "@langchain/core": "^1.0.4",
    "idb": "^8.0.3",
    "openai": "^4.52.7",
    "react": "^19.2.0",
    "react-dom": "^19.2.0",
    "react-masonry-css": "^1.0.16",
    "zustand": "^4.5.4",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@types/node": "^22.14.0",
    "@vitejs/plugin-react": "^5.0.0",
    "tsx": "^4.20.6",
    "typescript": "~5.8.2",
    "vite": "^6.2.0"
  }
}
